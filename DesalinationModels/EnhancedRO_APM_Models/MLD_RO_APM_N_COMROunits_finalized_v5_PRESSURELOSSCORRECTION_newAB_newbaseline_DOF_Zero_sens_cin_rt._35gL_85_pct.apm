     Xfj[1:N][1::Nstage]= (Mfj[0:N-1][1::Nstage]*Xfj[0:N-1][1::Nstage] + Lj[1::Nstage]*Wj[1::Nstage]/N*Jsj[1:N][1::Nstage])/Mfj[1:N][1::Nstage]
	 
	
	 
