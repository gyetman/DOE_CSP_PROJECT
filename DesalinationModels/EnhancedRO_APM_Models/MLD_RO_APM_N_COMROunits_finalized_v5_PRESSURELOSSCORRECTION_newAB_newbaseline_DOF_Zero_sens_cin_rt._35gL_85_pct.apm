	 SECsum[2:Nstage]=SECsum[1:Nstage-1]  +  SEC_Hpumpk[2:Nstage] + SEC_Bpumpk[2:Nstage] + SEC_Hpumpj[2:Nstage] + SEC_Bpumpj[2:Nstage] + SEC_Lpumpj[2:Nstage]
